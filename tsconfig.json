{
  "extends": "astro/tsconfigs/base",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": true,
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "preserve"
  }
}
